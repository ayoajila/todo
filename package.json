{
  "name": "todo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "audit": "npm audit",
    "lint": "node node_modules/eslint/bin/eslint.js config routes bin/www app.js",
    "test": "node node_modules/jest/bin/jest.js",
    "outdated": "npm outdated",
    "dev": "NODE_ENV=development nodemon --max-http-header-size=40000 ./bin/www",
    "all": "npm-run-all --parallel lint outdated audit test",
    "start": "node --max-http-header-size=40000 ./bin/www"
  },
  "dependencies": {
    "app-root-path": "^3.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "helmet": "^3.21.2",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "path": "^0.12.7",
    "pug": "^3.0.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^24.8.0",
    "nodemon": "^2.0.1",
    "set-cookie-parser": "^2.4.0",
    "supertest": "^4.0.2",
    "npm-run-all": "^4.1.5"
